
<div class="well col-md-12" id="DiscountSubTypeEditor" data-bind="visible: isDiscountSubTypesEditorVisible, with: selectedDiscountSubType " style="display: none; margin-top:15px">
    <div class="float-left detailsContainer">
        <h3 class="page-header detailsHeaderSection">@Cares.Web.Resources.Pricing.DiscountSubType.DiscountSubTypeDetail</h3>
        <button class="btn  btn-info" id="saveFleetPoolbtn" data-bind="click: $root.onSavebtn">@Cares.Web.Resources.Shared.Common.Save</button>
        <button class="btn  btn-default" id="cancelOrgGroupbtn" data-bind="click: $root.onCancelbtn">@Cares.Web.Resources.Shared.Common.Cancel</button>
    </div>
    <form class="form-horizontal">
        <div class="form-group">
            <label class="control-label h5 col-sm-2">@Cares.Web.Resources.Pricing.DiscountSubType.Code*</label>
            <div class="col-md-3">
                <input type="text" class="form-control" placeholder="@("<" + Cares.Web.Resources.Pricing.DiscountSubType.Code + ">")" data-bind="value: code" />
            </div>
            <label class="control-label h5 col-sm-1">@Cares.Web.Resources.Pricing.DiscountSubType.Name*</label>
            <div class="col-md-3">
                <input type="text" class="form-control" placeholder="@("<" + Cares.Web.Resources.Pricing.DiscountSubType.Name + ">")" data-bind="value:name" />
            </div>

        </div>
        <div class="form-group">
            <label class="control-label h5 col-sm-2">@Cares.Web.Resources.Pricing.DiscountSubType.DiscountType*</label>
            <div class="col-md-3">
                <select class="form-control" data-bind="options: $root.baseDiscountTypeList, optionsText: 'DiscountTypeCodeName', optionsValue: 'DiscountTypeId', optionsCaption: '&lt;' + '@Cares.Web.Resources.Pricing.DiscountSubType.DiscountType'+ '&gt;' , value:discountTypeId "></select>
            </div>
        </div>
        <div class="form-group">
            <label class="control-label h5 col-sm-2">@Cares.Web.Resources.Pricing.DiscountSubType.Description</label>
            <div class="col-md-3">
                <textarea class="form-control" rows="4" placeholder="@("<" + Cares.Web.Resources.Pricing.DiscountSubType.Description + ">")" data-bind="value: description"></textarea>
            </div>
        </div>

    </form>
</div>
