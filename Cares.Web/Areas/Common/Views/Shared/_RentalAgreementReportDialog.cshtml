        <div class="modal-content">
            <div class="modal-header">
                <h2 class="modal-title" id="myModalLabel">@Cares.Web.Resources.RentalAgreement.RentalAgreement.RentalAgreementReport_Title</h2>
            </div>
            <div class="modal-body">
                <div class="row">
                    <div class="well">
                        @Html.Partial("~/Areas/Common/Views/Shared/_RentalAgreementReport.cshtml")
