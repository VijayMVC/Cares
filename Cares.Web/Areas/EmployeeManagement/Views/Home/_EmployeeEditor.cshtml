<div class="well col-md-12" id="employeeEditor" data-bind="visible: isEmployeeEditorVisible, with: addEmployeeItem" style="display: none;">
    <div class="detailsContainer">
        <h3 class="page-header detailsHeaderSection">@Cares.Web.Resources.EmployeeManagement.Employee.EmployeeDetail </h3>
        <button class="btn  btn-info" id="saveBtn" data-bind="click: $root.onSaveEmployee, enable: hasChanges, css: { 'btn-warning': hasChanges }">@Cares.Web.Resources.Shared.Common.Save</button>
        <button class="btn btn-default" id="saveBtn" data-bind="click: $root.closeEmployeeEditor">@Cares.Web.Resources.Shared.Common.Cancel</button>
    </div>
    <form class="form-horizontal">
        <div class="form-group">
        </div>
        <div class="form-group">
            <label for="code" class="control-label h5 col-sm-1">@Cares.Web.Resources.EmployeeManagement.Employee.Code*</label>
            <div class="col-md-3">
                <input type="text" class="form-control" placeholder='@("<" + Cares.Web.Resources.EmployeeManagement.Employee.Code + ">")' id="code" data-bind="value:code , validationTooltip:code" />
            </div>
            <label for="company" class="control-label h5 col-sm-1">@Cares.Web.Resources.EmployeeManagement.Employee.Company *</label>
            <div class="col-md-3">
                <select id="company" class="form-control" data-bind="options: $root.companies, optionsText: 'CompanyCodeName', optionsValue: 'CompanyId', optionsCaption: '&lt;' + '@Cares.Web.Resources.EmployeeManagement.Employee.Company' + '&gt;' ,value:companyId, event:{change:$root.onSelectedCompany } "></select>
            </div>
            <label for="status" class="control-label h5 col-sm-1">@Cares.Web.Resources.EmployeeManagement.Employee.Status * </label>
            <div class="col-md-3">
                <select id="status" class="form-control" data-bind="options: $root.employeeStatuses, optionsText: 'EmpStatusCodeName', optionsValue: 'EmpStatusId', optionsCaption: '&lt;' + '@Cares.Web.Resources.EmployeeManagement.Employee.Status' + '&gt;' , value:statusId"></select>
            </div>
        </div>
        <div class="form-group">
            <label for="fName" class="control-label h5 col-sm-1">@Cares.Web.Resources.EmployeeManagement.Employee.FirstName *</label>
            <div class="col-md-3">
                <input type="text" class="form-control" placeholder='@("<"+Cares.Web.Resources.EmployeeManagement.Employee.FirstName+">")' id="fName" data-bind="value:fName , validationTooltip:fName" />
            </div>
            <label for="mName" class="control-label h5 col-sm-1">@Cares.Web.Resources.EmployeeManagement.Employee.MiddleName </label>
            <div class="col-md-3">
                <input type="text" class="form-control" placeholder='@("<"+Cares.Web.Resources.EmployeeManagement.Employee.MiddleName+">")' id="mName" data-bind="value:mName , validationTooltip:mName" />
            </div>
            <label for="lName" class="control-label h5 col-sm-1">@Cares.Web.Resources.EmployeeManagement.Employee.LastName </label>
            <div class="col-md-3">
                <input type="text" class="form-control" placeholder='@("<"+Cares.Web.Resources.EmployeeManagement.Employee.LastName+">")' id="lName" data-bind="value:lName , validationTooltip:lName" />
            </div>
        </div>
        <div class="form-group">
            <label for="dOB" class="control-label h5 col-sm-1">@Cares.Web.Resources.EmployeeManagement.Employee.DOB *</label>
            <div class="col-md-3">
                <input type="text" class="form-control" placeholder='@("<"+Cares.Web.Resources.EmployeeManagement.Employee.DOB+">")' id="dOB" data-bind="datepicker:dOB , validationTooltip:dOB" />
            </div>
            <label for="company" class="control-label h5 col-sm-1">@Cares.Web.Resources.EmployeeManagement.Employee.Gender </label>
            <div class="col-md-3">
                <select id="company" class="form-control" data-bind="options: $root.genders, optionsText: 'Text', optionsValue: 'Id', optionsCaption: '&lt;' + '@Cares.Web.Resources.EmployeeManagement.Employee.Gender' + '&gt;' ,value:gender "></select>
            </div>
            <label for="nationality" class="control-label h5 col-sm-1"> @Cares.Web.Resources.EmployeeManagement.Employee.Nationality </label>
            <div class="col-md-3">
                <select id="nationality" class="form-control" data-bind="options: $root.nationalities, optionsText: 'CountryCodeName', optionsValue: 'CountryId', optionsCaption: '&lt;' + '@Cares.Web.Resources.EmployeeManagement.Employee.Nationality' + '&gt;' , value:nationalityId"></select>
            </div>
        </div>
        <div class="form-group">
            <label for="notes" class="control-label h5 col-sm-1">@Cares.Web.Resources.EmployeeManagement.Employee.Notes</label>
            <div class="col-md-3">
                <textarea class="form-control" rows="4" placeholder='@("<"+Cares.Web.Resources.EmployeeManagement.Employee.Notes+">")' id="notes" data-bind="value: notes , valueUpdate: 'keyup'"></textarea>
            </div>
            <label for="notes1" class="control-label h5 col-sm-1">@Cares.Web.Resources.EmployeeManagement.Employee.Notes1</label>
            <div class="col-md-3">
                <textarea class="form-control" rows="4" placeholder='@("<"+Cares.Web.Resources.EmployeeManagement.Employee.Notes1+">")' id="notes1" data-bind="value: notes1 , valueUpdate: 'keyup'"></textarea>
            </div>
            <label for="notes2" class="control-label h5 col-sm-1">@Cares.Web.Resources.EmployeeManagement.Employee.Notes 2</label>
            <div class="col-md-3">
                <textarea class="form-control" rows="4" placeholder='@("<"+Cares.Web.Resources.EmployeeManagement.Employee.Notes2+">")' id="notes2" data-bind="value: notes2 , valueUpdate: 'keyup'"></textarea>
            </div>
        </div>
        <div class="form-group">
            <label for="notes3" class="control-label h5 col-sm-1">@Cares.Web.Resources.EmployeeManagement.Employee.Notes3</label>
            <div class="col-md-3">
                <textarea class="form-control" rows="4" placeholder='@("<"+Cares.Web.Resources.EmployeeManagement.Employee.Notes3+">")' id="notes3" data-bind="value: notes3 , valueUpdate: 'keyup'"></textarea>
            </div>
            <label for="notes4" class="control-label h5 col-sm-1">@Cares.Web.Resources.EmployeeManagement.Employee.Notes4</label>
            <div class="col-md-3">
                <textarea class="form-control" rows="4" placeholder='@("<"+Cares.Web.Resources.EmployeeManagement.Employee.Notes4+">")' id="notes4" data-bind="value: notes4 , valueUpdate: 'keyup'"></textarea>
            </div>
            <label for="notes5" class="control-label h5 col-sm-1">@Cares.Web.Resources.EmployeeManagement.Employee.Notes5</label>
            <div class="col-md-3">
                <textarea class="form-control" rows="4" placeholder='@("<"+Cares.Web.Resources.EmployeeManagement.Employee.Notes5+">")' id="notes5" data-bind="value: notes5 , valueUpdate: 'keyup'"></textarea>
            </div>
        </div>
        <br />
        <div class="panel panel-info">
            <div class="panel-heading">.</div>
            <!-- Nav tabs -->
            <ul class="nav nav-tabs" role="tablist">
                <li class="active"><a href="#jobInfo" role="tab" data-toggle="tab">@Cares.Web.Resources.EmployeeManagement.Employee.JobInfo</a></li>
                <li><a href="#address" role="tab" data-toggle="tab">@Cares.Web.Resources.EmployeeManagement.Employee.Address</a></li>
                <li><a href="#phone" role="tab" data-toggle="tab">@Cares.Web.Resources.EmployeeManagement.Employee.Phone</a></li>
                <li><a href="#documentInfo" role="tab" data-toggle="tab">@Cares.Web.Resources.EmployeeManagement.Employee.DocumentInfo</a></li>
                <li><a href="#jobProgress" role="tab" data-toggle="tab">@Cares.Web.Resources.EmployeeManagement.Employee.JobProgress</a></li>
                <li><a href="#authorizedLocations" role="tab" data-toggle="tab">@Cares.Web.Resources.EmployeeManagement.Employee.AuthorizedLocations</a></li>
            </ul>
            <!-- Tab panes -->
            <div class="tab-content">
                <br />
                <!-- first tab - Job Info.-->
                <div class="tab-pane active" id="jobInfo" data-bind="with: jobInfo">
                    <div class="form-group">
                        <label for="jobTypes" class="control-label h5 col-sm-1">@Cares.Web.Resources.EmployeeManagement.Employee.JobType * </label>
                        <div class="col-md-3">
                            <select id="jobTypes" class="form-control" data-bind="options: $root.jobTypes, optionsText: 'JobTypeCodeName', optionsValue: 'JobTypeId', optionsCaption: '&lt;' + '@Cares.Web.Resources.EmployeeManagement.Employee.JobType' + '&gt;' ,value:jobTypeId "></select>
                        </div>
                        <label for="designation" class="control-label h5 col-sm-1">@Cares.Web.Resources.EmployeeManagement.Employee.Designation * </label>
                        <div class="col-md-3">
                            <select id="designation" class="form-control" data-bind="options: $root.designations, optionsText: 'DesignationCodeName', optionsValue: 'DesignationId', optionsCaption: '&lt;' + '@Cares.Web.Resources.EmployeeManagement.Employee.Designation' + '&gt;' , value:desgId"></select>
                        </div>
                        <label for="desgGrade" class="control-label h5 col-sm-1"> @Cares.Web.Resources.EmployeeManagement.Employee.DesignationGrade * </label>
                        <div class="col-md-3">
                            <select id="desgGrade" class="form-control" data-bind="options: $root.designationGrades, optionsText: 'DesigGradeCodeName', optionsValue: 'DesigGradeId', optionsCaption: '&lt;' + '@Cares.Web.Resources.EmployeeManagement.Employee.DesignationGrade' + '&gt;' , value:desgGardeId"></select>
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="jobTypes" class="control-label h5 col-sm-1">@Cares.Web.Resources.EmployeeManagement.Employee.Department *</label>
                        <div class="col-md-3">
                            <select id="jobTypes" class="form-control" data-bind="options: $root.filteredDepartments, optionsText: 'DepartmentCodeName', optionsValue: 'DepartmentId', optionsCaption: '&lt;' + '@Cares.Web.Resources.EmployeeManagement.Employee.Department' + '&gt;' ,value:depId "></select>
                        </div>
                        <label for="designation" class="control-label h5 col-sm-1">@Cares.Web.Resources.EmployeeManagement.Employee.Workplace * </label>
                        <div class="col-md-3">
                            <select id="designation" class="form-control" data-bind="options: $root.filteredWorkplaces, optionsText: 'WorkPlaceCodeName', optionsValue: 'WorkPlaceId', optionsCaption: '&lt;' + '@Cares.Web.Resources.EmployeeManagement.Employee.Workplace' + '&gt;' , value:workPlaceId"></select>
                        </div>
                        <label for="supervisors" class="control-label h5 col-sm-1"> @Cares.Web.Resources.EmployeeManagement.Employee.Supervisor  </label>
                        <div class="col-md-3">
                            <select id="supervisors" class="form-control" data-bind="options: $root.supervisors, optionsText: 'EmployeeCodeName', optionsValue: 'EmployeeId', optionsCaption: '&lt;' + '@Cares.Web.Resources.EmployeeManagement.Employee.Supervisor' + '&gt;' , value:supervisorId"></select>
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="joiningDate" class="control-label h5 col-sm-1">@Cares.Web.Resources.EmployeeManagement.Employee.JoiningDate *</label>
                        <div class="col-md-3">
                            <input type="text" class="form-control" placeholder='@("<"+Cares.Web.Resources.EmployeeManagement.Employee.JoiningDate+">")' id="joiningDate" data-bind="datepicker:joiningDate , validationTooltip:joiningDate" />
                        </div>
                        <label for="salary" class="control-label h5 col-sm-1">@Cares.Web.Resources.EmployeeManagement.Employee.Salary</label>
                        <div class="col-md-3">
                            <input type="text" class="form-control" placeholder='@("<"+Cares.Web.Resources.EmployeeManagement.Employee.Salary+">")' id="salary" data-bind="value:salary , validationTooltip:salary" />
                        </div>
                    </div>
                </div>
                <!-- second tab - Address-->
                <div class="tab-pane" id="address">
                    <div data-bind="with:address">
                        <div class="addToBtnDiv">
                            <div class="addToBtnDiv row"><button class="btn btn-info addToBtn" id="saveBtn" data-bind="click: $root.onAddAddress">@Cares.Web.Resources.Shared.Common.AddToList</button></div>
                        </div>
                        <div class="form-group">
                            <label for="country" class="control-label h5 col-sm-1">@Cares.Web.Resources.EmployeeManagement.Employee.Country * </label>
                            <div class="col-md-3">
                                <select id="country" class="form-control" data-bind="options: $root.countries, optionsText: 'CountryCodeName', optionsValue: 'CountryId', optionsCaption: '&lt;' + '@Cares.Web.Resources.EmployeeManagement.Employee.Country' + '&gt;' ,value:countryId,event:{change:$root.onCountrySelectionChange} "></select>
                            </div>
                            <label for="region" class="control-label h5 col-sm-1"> @Cares.Web.Resources.EmployeeManagement.Employee.Region </label>
                            <div class="col-md-3">
                                <select id="region" class="form-control" data-bind="options: $root.filteredRegions, optionsText: 'RegionCodeName', optionsValue: 'RegionId', optionsCaption: '&lt;' + '@Cares.Web.Resources.EmployeeManagement.Employee.Region' + '&gt;' , value:regionId,event:{change:$root.onRegionSelectionChange}"></select>
                            </div>
                            <label for="subRegion" class="control-label h5 col-sm-1"> @Cares.Web.Resources.EmployeeManagement.Employee.SubRegion </label>
                            <div class="col-md-3">
                                <select id="subRegion" class="form-control" data-bind="options: $root.filteredSubRegions, optionsText: 'SubRegionCodeName', optionsValue: 'SubRegionId', optionsCaption: '&lt;' + '@Cares.Web.Resources.EmployeeManagement.Employee.SubRegion' + '&gt;' , value:subRegionId,event:{change:$root.onSubRegionSelectionChange}"></select>
                            </div>
                        </div>
                        <div class="form-group">
                            <label for="city" class="control-label h5 col-sm-1">@Cares.Web.Resources.EmployeeManagement.Employee.City</label>
                            <div class="col-md-3">
                                <select id="city" class="form-control" data-bind="options: $root.filteredCities, optionsText: 'CityCodeName', optionsValue: 'CityId', optionsCaption: '&lt;' + '@Cares.Web.Resources.EmployeeManagement.Employee.City' + '&gt;' ,value:cityId,event:{change:$root.onCitySelectionChange} "></select>
                            </div>
                            <label for="area" class="control-label h5 col-sm-1">@Cares.Web.Resources.EmployeeManagement.Employee.Area</label>
                            <div class="col-md-3">
                                <select id="area" class="form-control" data-bind="options: $root.filteredAreas, optionsText: 'AreaCodeName', optionsValue: 'AreaId', optionsCaption: '&lt;' + '@Cares.Web.Resources.EmployeeManagement.Employee.Area' + '&gt;' , value:areaId,event:{change:$root.onAreaSelectionChange}"></select>
                            </div>
                            <label for="poBox" class="control-label h5 col-sm-1">@Cares.Web.Resources.EmployeeManagement.Employee.POBoxNumber</label>
                            <div class="col-md-3">
                                <input type="text" class="form-control" placeholder='@("<"+Cares.Web.Resources.EmployeeManagement.Employee.POBoxNumber+">")' id="poBox" data-bind="value:poBox , validationTooltip:poBox" />
                            </div>
                        </div>
                        <div class="form-group">
                            <label for="addressType" class="control-label h5 col-sm-1">@Cares.Web.Resources.EmployeeManagement.Employee.AddressType *</label>
                            <div class="col-md-3">
                                <select id="addressType" class="form-control" data-bind="options: $root.addressTypes, optionsText: 'AddressTypeCodeName', optionsValue: 'AddressTypeId', optionsCaption: '&lt;' + '@Cares.Web.Resources.EmployeeManagement.Employee.AddressType' + '&gt;' ,value:addressTypeId,event:{change:$root.onAddressTypeSelectionChange} "></select>
                            </div>
                            <label for="zipCode" class="control-label h5 col-sm-1">@Cares.Web.Resources.EmployeeManagement.Employee.ZipCode </label>
                            <div class="col-md-3">
                                <input type="text" class="form-control" placeholder='@("<"+Cares.Web.Resources.EmployeeManagement.Employee.ZipCode+">")' id="zipCode" data-bind="value:zipCode , validationTooltip:zipCode" />
                            </div>
                            <label for="conPerson" class="control-label h5 col-sm-1">@Cares.Web.Resources.EmployeeManagement.Employee.ContactPerson</label>
                            <div class="col-md-3">
                                <input type="text" class="form-control" placeholder='@("<"+Cares.Web.Resources.EmployeeManagement.Employee.ContactPerson+">")' id="conPerson" data-bind="value:contactPerson , validationTooltip:contactPerson" />
                            </div>
                        </div>
                        <div class="form-group">
                            <label for="stAddress" class="control-label h5 col-sm-1">@Cares.Web.Resources.EmployeeManagement.Employee.StreetAddress *</label>
                            <div class="col-md-3">
                                <textarea class="form-control" rows="4" placeholder='@("<"+Cares.Web.Resources.EmployeeManagement.Employee.StreetAddress+">")' id="stAddress" data-bind="value: streetAddress, valueUpdate: 'keyup'"></textarea>
                            </div>
                            <label for="email" class="control-label h5 col-sm-1">@Cares.Web.Resources.EmployeeManagement.Employee.EmailAddress </label>
                            <div class="col-md-3">
                                <input type="text" class="form-control" placeholder='@("<"+Cares.Web.Resources.EmployeeManagement.Employee.EmailAddress+">")' id="email" data-bind="value:emailAddress , validationTooltip:emailAddress" />
                            </div>
                            <label for="webPage" class="control-label h5 col-sm-1">@Cares.Web.Resources.EmployeeManagement.Employee.WebPage</label>
                            <div class="col-md-3">
                                <input type="text" class="form-control" placeholder='@("<"+Cares.Web.Resources.EmployeeManagement.Employee.WebPage+">")' id="webPage" data-bind="value:webPage , validationTooltip:webPage" />
                            </div>
                        </div>
                    </div>
                    <br/>
                    <div class="form-group">
                        <div class=" col-md-12 well emloyeeInnerWell">
                            <table id="addressTable" class="table table-condensed">
                                <thead>
                                    <tr>
                                        <th> <span id="1">@Cares.Web.Resources.EmployeeManagement.Employee.AddressType<span></span></span></th>
                                        <th> <span id="2">@Cares.Web.Resources.EmployeeManagement.Employee.Country<span></span></span></th>
                                        <th> <span id="3">@Cares.Web.Resources.EmployeeManagement.Employee.Region<span></span></span></th>
                                        <th> <span id="4">@Cares.Web.Resources.EmployeeManagement.Employee.SubRegion<span></span></span></th>
                                        <th> <span id="5">@Cares.Web.Resources.EmployeeManagement.Employee.City<span></span></span></th>
                                        <th> <span id="6">@Cares.Web.Resources.EmployeeManagement.Employee.Area<span></span></span></th>
                                        <th> <span id="7">@Cares.Web.Resources.EmployeeManagement.Employee.POBoxNumber<span></span></span></th>
                                        <th> <span id="8">@Cares.Web.Resources.EmployeeManagement.Employee.ZipCode<span></span></span></th>
                                        <th> <span id="9">@Cares.Web.Resources.EmployeeManagement.Employee.WebAddress<span></span></span></th>
                                        <th> <span id="10">@Cares.Web.Resources.EmployeeManagement.Employee.EmailAddress<span></span></span></th>
                                        <th> <span id="11">@Cares.Web.Resources.EmployeeManagement.Employee.Address<span></span></span></th>
                                        <th> <span id="12">@Cares.Web.Resources.EmployeeManagement.Employee.ContactPerson<span></span></span></th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <!-- ko template: { name: 'itemAddressTemplate' , foreach: $root.addressList } -->
                                    <!-- /ko -->
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
                <!-- third tab - Phone-->
                <div class="tab-pane" id="phone">
                    <div data-bind="with:phone">
                        <div class="addToBtnDiv">
                            <div class="addToBtnDiv row"><button class="btn btn-info addToBtn" id="saveBtn" data-bind="click: $root.onAddEmployeePhone">@Cares.Web.Resources.Shared.Common.AddToList</button></div>
                        </div>
                        <div class="form-group">
                            <label for="phoneTypes" class="control-label h5  col-sm-2">@Cares.Web.Resources.EmployeeManagement.Employee.PhoneType *</label>
                            <div class="col-md-2">
                                <select id="phoneTypes" class="form-control" data-bind="options: $root.phoneTypes, optionsText: 'PhoneTypeCodeName', optionsCaption: '&lt;' + '@Cares.Web.Resources.EmployeeManagement.Employee.PhoneType' + '&gt;',value:phoneTypeId"></select>
                            </div>
                            <label for="phoneNumber" class="control-label h5  col-sm-2">@Cares.Web.Resources.EmployeeManagement.Employee.PhoneNumber *</label>
                            <div class="col-md-2">
                                <input type="text" class="form-control" placeholder='@("<"+Cares.Web.Resources.EmployeeManagement.Employee.PhoneNumber+">")' id="phoneNumber" data-bind="value: phoneNumber, valueUpdate: 'keyup'" />
                            </div>
                            <label for="isDefault" class="control-label  h5 col-sm-2">@Cares.Web.Resources.EmployeeManagement.Employee.IsDefault</label>
                            <div class="col-md-2">
                                <input type="checkbox" class="form-control" id="isDefault" data-bind="checked: isDefault">
                            </div>
                        </div>

                    </div>
                    <br />
                    <div class="form-group">
                        <div class=" col-md-8 well emloyeeInnerWell">
                            <table id="phonesTable" class="table table-condensed">
                                <thead>
                                    <tr>
                                        <th> <span id="1">@Cares.Web.Resources.EmployeeManagement.Employee.PhoneType<span></span></span></th>
                                        <th> <span id="2">@Cares.Web.Resources.EmployeeManagement.Employee.PhoneNumber<span></span></span></th>
                                        <th> <span id="3">@Cares.Web.Resources.EmployeeManagement.Employee.IsDefault<span></span></span></th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <!-- ko template: { name: 'itemEmployeePhoneTemplate' , foreach: $root.phoneNumbersList } -->
                                    <!-- /ko -->
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
                <!-- fourth tab - Document Info.-->
                <div class="tab-pane" id="documentInfo">
                    <div data-bind="with:documentInfo">
                        <div class="form-group">
                            <label for="nic" class="control-label h5 col-sm-1">@Cares.Web.Resources.EmployeeManagement.Employee.NIC</label>
                            <div class="col-md-3">
                                <input id="nic" type="text" class="form-control" placeholder='@("<"+Cares.Web.Resources.EmployeeManagement.Employee.NIC+">")' data-bind="value:nic , validationTooltip:nic" />
                            </div>
                            <label for="nicExpDt" class="control-label h5 col-sm-1">@Cares.Web.Resources.EmployeeManagement.Employee.NICExpDate </label>
                            <div class="col-md-3">
                                <input id="nicExpDt" type="text" class="form-control" placeholder='@("<"+Cares.Web.Resources.EmployeeManagement.Employee.NICExpDate+">")' data-bind="datepicker:nicExpDt , validationTooltip:nicExpDt" />
                            </div>
                        </div>
                        <div class="form-group">
                            <label for="iqamaNum" class="control-label h5 col-sm-1">@Cares.Web.Resources.EmployeeManagement.Employee.IqamaNumber </label>
                            <div class="col-md-3">
                                <input id="iqamaNum" type="text" class="form-control" placeholder='@("<"+Cares.Web.Resources.EmployeeManagement.Employee.IqamaNumber+">")' data-bind="value:iqamaNum , validationTooltip:iqamaNum" />
                            </div>
                            <label for="iqamaExpDt" class="control-label h5 col-sm-1">@Cares.Web.Resources.EmployeeManagement.Employee.IqamaExpiryDate </label>
                            <div class="col-md-3">
                                <input id="iqamaExpDt" type="text" class="form-control" placeholder='@("<"+Cares.Web.Resources.EmployeeManagement.Employee.IqamaExpiryDate+">")' data-bind="datepicker:iqamaExpDt , validationTooltip:iqamaExpDt" />
                            </div>
                        </div>
                        <div class="form-group">
                            <label for="passportNum" class="control-label h5 col-sm-1">@Cares.Web.Resources.EmployeeManagement.Employee.PassportNumber  </label>
                            <div class="col-md-3">
                                <input id="passportNum" type="text" class="form-control" placeholder='@("<"+Cares.Web.Resources.EmployeeManagement.Employee.PassportNumber+">")' data-bind="value:passportNum , validationTooltip:passportNum" />
                            </div>
                            <label for="passportExpDt" class="control-label h5 col-sm-1">@Cares.Web.Resources.EmployeeManagement.Employee.PassportExpiryDate </label>
                            <div class="col-md-3">
                                <input id="passportExpDt" type="text" class="form-control" placeholder='@("<"+Cares.Web.Resources.EmployeeManagement.Employee.PassportExpiryDate+">")' data-bind="datepicker:passportExpDt , validationTooltip:passportExpDt" />
                            </div>
                            <label for="passportCountry" class="control-label h5 col-sm-1">@Cares.Web.Resources.EmployeeManagement.Employee.PassportCountry </label>
                            <div class="col-md-3">
                                <select id="passportCountry" class="form-control" data-bind="options: $root.countries, optionsText: 'CountryCodeName', optionsValue: 'CountryId', optionsCaption: '&lt;' + '@Cares.Web.Resources.EmployeeManagement.Employee.PassportCountry' + '&gt;' ,value:passCountryId "></select>
                            </div>
                        </div>
                        <div class="form-group">
