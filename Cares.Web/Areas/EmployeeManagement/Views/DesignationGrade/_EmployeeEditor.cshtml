                </div>
                <!-- sixth tab - Authorized Locations-->
                <div class="tab-pane" id="authorizedLocations">
                    <div data-bind="with:authorizedLocation">
                        <div class="form-group">
                            <div class="col-md-2">
                                <button class="btn btn-sm pull-right" id="addJobProgress" data-bind="click: $root.onAddAuthorizedLocation">Add To List</button>
                            </div>
                        </div>
                        <div class="form-group">
                            <label for="operation" class="control-label h5 col-sm-1"> Operation * </label>
                            <div class="col-md-3">
                                <select id="operation" class="form-control" data-bind="options: $root.filteredOperations, optionsText: 'OperationCodeName', optionsCaption: '&lt;' + 'Operation' + '&gt;' , value:operationId, event:{change:$root.onSelectedOperation }"></select>
                            </div>
                            <label for="defaultOpr" class="control-label  h5 col-sm-2">Default Operation? </label>
                            <div class="col-md-2">
                                <input type="checkbox" class="form-control" id="defaultOpr" data-bind="checked: isDefault">
                            </div>
                        </div>
                        <div class="form-group">
                            <label for="operationWorkplaces" class="control-label h5 col-sm-1">Operation Workplace * </label>
                            <div class="col-md-3">
                                <select id="operationWorkplaces" class="form-control" data-bind="options: $root.filteredOperationsWorkplaces, optionsText: 'OperationsWorkPlaceCodeName', optionsCaption: '&lt;' + 'Operation Workplace' + '&gt;' , value:operationsWorkplaceId"></select>
                            </div>
                            <label for="defaultOprWP" class="control-label  h5 col-sm-2">Default Operation Workplace? </label>
                            <div class="col-md-2">
                                <input type="checkbox" class="form-control" id="defaultOprWP" data-bind="checked: isOperationDefault">
                            </div>
                        </div>
                    </div>
                    <div class="form-group">
                        <div class=" col-md-8 well">
                            <table id="authLocationTable" class="table table-condensed">
                                <thead>
                                    <tr>
                                        <th> <span id="1">Operation<span></span></span></th>
                                        <th> <span id="2">Default Operation?<span></span></span></th>
                                        <th> <span id="3">Operations Workpace<span></span></span></th>
                                        <th> <span id="3">Default Operations Workpace<span></span></span></th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <!-- ko template: { name: 'itemAuthLocationTemplate' , foreach: $root.authLocationsList } -->
                                    <!-- /ko -->
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </form>
</div>
