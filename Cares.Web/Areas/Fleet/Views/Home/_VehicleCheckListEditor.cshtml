
<div class="well col-md-12" id="VehicleCheckListEditor" data-bind="visible: isVehicleCheckListEditorVisible, with: selectedVehicleCheckList " style="display: none; margin-top:15px">
    <div class="float-left detailsContainer">
        <h3 class="page-header detailsHeaderSection">@Cares.Web.Resources.FleetPool.VehicleCheckList.VehicleCheckListDetail</h3>
        <button class="btn  btn-info" id="saveFleetPoolbtn" data-bind="click: $root.onSavebtn">@Cares.Web.Resources.Shared.Common.Save</button>
        <button class="btn  btn-default" id="cancelOrgGroupbtn" data-bind="click: $root.onCancelbtn">@Cares.Web.Resources.Shared.Common.Cancel</button>
    </div>
    <form class="form-horizontal">
        <div class="form-group">
            <label class="control-label h5 col-sm-2">@Cares.Web.Resources.FleetPool.VehicleCheckList.Code*</label>
            <div class="col-md-3">
                <input type="text" class="form-control" placeholder="@("<" + Cares.Web.Resources.FleetPool.VehicleCheckList.Code + ">")" data-bind="value: code" />
            </div>
            <label class="control-label h5 col-sm-1">@Cares.Web.Resources.FleetPool.VehicleCheckList.Name*</label>
            <div class="col-md-3">
                <input type="text" class="form-control" placeholder="@("<" + Cares.Web.Resources.FleetPool.VehicleCheckList.Name + ">")" data-bind="value:name" />
            </div>
        </div>

        <div class="form-group">
            <label class="control-label h5 col-sm-2">@Cares.Web.Resources.FleetPool.VehicleCheckList.Description</label>
            <div class="col-md-3">
                <textarea class="form-control" rows="4" placeholder="@("<" + Cares.Web.Resources.FleetPool.VehicleCheckList.Description + ">")" data-bind="value: description"></textarea>
            </div>
            <label class="control-label h5 col-sm-1">@Cares.Web.Resources.FleetPool.VehicleCheckList.IsInterior*</label>
            <div class="col-md-3">
                <input type="checkbox" class="form-control" data-bind="checked:isInterior" />
            </div>
        </div>
    </form>
</div>
