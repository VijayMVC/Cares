<div class="well col-md-12" id="vehicleEditor" data-bind="visible: isVehicleEditorVisible, with: addVehicleItem" style="display: none;">
    <div class="detailsContainer">
        <h3 class="page-header detailsHeaderSection">@Cares.Web.Resources.Vehicle.Vehicle.VehicleDetail </h3>
        <button class="btn  btn-info" id="saveBtn" data-bind="click: $root.onSaveVehicle, enable: hasChanges, css: { 'btn-warning': hasChanges }">@Cares.Web.Resources.Shared.Common.Save</button>
        <button class="btn btn-default" id="saveBtn" data-bind="click: $root.closeVehicleEditor">@Cares.Web.Resources.Shared.Common.Cancel</button>
    </div>
    <form class="form-horizontal">
        <div class="form-group">
            <label for="plateNumber" class="control-label h5 col-sm-2">@Cares.Web.Resources.Vehicle.Vehicle.PlateNumber *</label>
            <div class="col-md-2">
                <input type="text" class="form-control" placeholder='@("<" +Cares.Web.Resources.Vehicle.Vehicle.PlateNumber + ">")' id="plateNumber" data-bind="value:plateNumber , validationTooltip:plateNumber ,disable :(vehicleId()!==0)" />
            </div>
            <label for="vName" class="control-label h5 col-sm-2">@Cares.Web.Resources.Vehicle.Vehicle.VehicleName </label>
            <div class="col-md-2">
                <input type="text" class="form-control" placeholder='@("<" + Cares.Web.Resources.Vehicle.Vehicle.VehicleName + ">")' id="vName" data-bind="value:vehicleName , validationTooltip:vehicleName " />
            </div>
            <label for="vPictorialCondition" class="control-label h5 col-sm-2">@Cares.Web.Resources.Vehicle.Vehicle.VehiclePictorialCondition </label>
            <div class="col-md-2">
                <input type="button" class="form-control" id="vPictorialCondition" />
            </div>
        </div>
        <div class="form-group">
            <label for="company" class="control-label h5 col-sm-2">@Cares.Web.Resources.Vehicle.Vehicle.Company </label>
            <div class="col-md-2">
                <select id="company" class="form-control" data-bind="options: $root.companies, optionsText: 'CompanyCodeName', optionsValue: 'CompanyId', optionsCaption: '&lt;' + '@Cares.Web.Resources.Vehicle.Vehicle.Company' + '&gt;' , value: companyId, validationTooltip: companyId, event:{change:$root.onSelectedCompany}"></select>
            </div>
            <label for="department" class="control-label h5 col-sm-2">@Cares.Web.Resources.Vehicle.Vehicle.Department </label>
            <div class="col-md-2">
                <select id="department" class="form-control" data-bind="options: $root.filteredDepartments, optionsText: 'DepartmentCodeName', optionsValue: 'DepartmentId', optionsCaption:  '&lt;' + '@Cares.Web.Resources.Vehicle.Vehicle.Department' + '&gt;' , value: departmentId, validationTooltip: departmentId, event:{change:$root.onSelectedDepartemnt}"></select>
            </div>
            <label for="operation" class="control-label h5 col-sm-2">@Cares.Web.Resources.Vehicle.Vehicle.Operation </label>
            <div class="col-md-2">
                <select id="operation" class="form-control" data-bind="options: $root.filteredOperations, optionsText: 'OperationCodeName', optionsValue: 'OperationId', optionsCaption: '&lt;' + '@Cares.Web.Resources.Vehicle.Vehicle.Operation' + '&gt;'  , value: operationId, validationTooltip: operationId"></select>
            </div>
        </div>
        <div class="form-group">
            <label for="region" class="control-label h5 col-sm-2">@Cares.Web.Resources.Vehicle.Vehicle.Region </label>
            <div class="col-md-2">
                <select id="region" class="form-control" data-bind="options: $root.regions, optionsText: 'RegionCodeName', optionsValue: 'RegionId', optionsCaption:  '&lt;' + '@Cares.Web.Resources.Vehicle.Vehicle.Region' + '&gt;', value: regionId, validationTooltip: regionId "></select>
            </div>
            <label for="fleetPool" class="control-label h5 col-sm-2">@Cares.Web.Resources.Vehicle.Vehicle.FleetPool </label>
            <div class="col-md-2">
                <select id="fleetPool" class="form-control" data-bind="options: $root.fleetPools, optionsText: 'FleetPoolCodeName', optionsValue: 'FleetPoolId', optionsCaption:  '&lt;' + '@Cares.Web.Resources.Vehicle.Vehicle.FleetPool' + '&gt;', value: fleetPoolId, validationTooltip: fleetPoolId"></select>
            </div>
            <label for="location" class="control-label h5 col-sm-2">@Cares.Web.Resources.Vehicle.Vehicle.AtLocation  </label>
            <div class="col-md-2">
                <select id="location" class="form-control" data-bind="options: $root.locations, optionsText: 'LocationCode', optionsValue: 'OperationsWorkPlaceId', optionsCaption: '&lt;' + '@Cares.Web.Resources.Vehicle.Vehicle.Location' + '&gt;', value: locationId, validationTooltip: locationId"></select>
            </div>
        </div>
        <div class="form-group">
            <label for="fuelType" class="control-label h5 col-sm-2">@Cares.Web.Resources.Vehicle.Vehicle.FuelType * </label>
            <div class="col-md-2">
                <select id="fuelType" class="form-control" data-bind="options: $root.fuelTypes, optionsText: 'FuelTypeCodeName', optionsValue: 'FuelTypeId', optionsCaption:  '&lt;' + '@Cares.Web.Resources.Vehicle.Vehicle.FuelType' + '&gt;', value: fuelTypeId, validationTooltip: fuelTypeId "></select>
            </div>
            <label class="control-label h5 col-sm-2">@Cares.Web.Resources.Vehicle.Vehicle.FuelLevel * </label>
            <div class="col-md-2">

            </div>
            <label for="vMake" class="control-label h5 col-sm-2">@Cares.Web.Resources.Vehicle.Vehicle.VehicleMake *  </label>
            <div class="col-md-2">
                <select id="vMake" class="form-control" data-bind="options: $root.vehicleMakes, optionsText: 'VehicleMakeCodeName', optionsValue: 'VehicleMakeId', optionsCaption:'&lt;' + '@Cares.Web.Resources.Vehicle.Vehicle.VehicleMake' + '&gt;', value: vehicleMakeId, validationTooltip: vehicleMakeId"></select>
            </div>
        </div>
        <div class="form-group">
            <label for="vModel" class="control-label h5 col-sm-2">@Cares.Web.Resources.Vehicle.Vehicle.VehicleModel * </label>
            <div class="col-md-2">
                <select id="vModel" class="form-control" data-bind="options: $root.vehicleModels, optionsText: 'VehicleModelCodeName', optionsValue: 'VehicleModeld', optionsCaption: 'Vehicle Model', value:vehicleModelId "></select>
            </div>
            <label for="vCategory" class="control-label h5 col-sm-2">@Cares.Web.Resources.Vehicle.Vehicle.VehicleCategory * </label>
            <div class="col-md-2">
                <select id="vCategory" class="form-control" data-bind="options: $root.vehicleCategories, optionsText: 'VehicleCategoryCodeName', optionsValue: 'VehicleCategoryId', optionsCaption: '&lt;' + '@Cares.Web.Resources.Vehicle.Vehicle.VehicleCategory' + '&gt;', value: vehicleCategoryId "></select>
            </div>
            <label for="modelYear" class="control-label h5 col-sm-2">@Cares.Web.Resources.Vehicle.Vehicle.ModelYear *  </label>
            <div class="col-md-2">
                <select id="modelYear" class="form-control" data-bind="options: $root.modelYears, optionsText: 'Text', optionsValue: 'Id', optionsCaption: '&lt;' + '@Cares.Web.Resources.Vehicle.Vehicle.ModelYear' + '&gt;', value: modelYear, validationTooltip: modelYear"></select>
            </div>
        </div>
        <div class="form-group">
            <label for="status" class="control-label h5 col-sm-2">@Cares.Web.Resources.Vehicle.Vehicle.Status * </label>
            <div class="col-md-2">
                <select id="status" class="form-control" data-bind="options: $root.vehicleStatuses, optionsText: 'VehicleStatusCodeName', optionsValue: 'VehicleStatusId', optionsCaption: '&lt;' + '@Cares.Web.Resources.Vehicle.Vehicle.Status' + '&gt;', value:statusId "></select>
            </div>
            <label for="transmission" class="control-label h5 col-sm-2">@Cares.Web.Resources.Vehicle.Vehicle.TransmissionType * </label>
            <div class="col-md-2">
                <select id="transmission" class="form-control" data-bind="options: $root.transmissionTypes, optionsText: 'TransmissionTypeCodeName', optionsValue: 'TransmissionTypeId', optionsCaption: '&lt;' + '@Cares.Web.Resources.Vehicle.Vehicle.TransmissionType' + '&gt;', value: transmissionTypeId "></select>
            </div>
            <label for="tankSize" class="control-label h5 col-sm-2">@Cares.Web.Resources.Vehicle.Vehicle.TankSize* </label>
            <div class="col-md-2">
                <input id="tankSize" type="text" class="form-control" placeholder='@("<" +Cares.Web.Resources.Vehicle.Vehicle.TankSize + ">")' data-bind="value:tankSize , validationTooltip:tankSize " />
            </div>
        </div>
        <div class="form-group">
            <label for="color" class="control-label h5 col-sm-2">@Cares.Web.Resources.Vehicle.Vehicle.Color *</label>
            <div class="col-md-2">
                <input id="color" type="text" class="form-control" placeholder='@("<" +Cares.Web.Resources.Vehicle.Vehicle.Color + ">")' data-bind="value:color , validationTooltip:color " />
            </div>
            <label for="initialOdemeter" class="control-label h5 col-sm-2">@Cares.Web.Resources.Vehicle.Vehicle.InitialOdometer * </label>
            <div class="col-md-2">
                <input id="initialOdemeter" type="text" class="form-control" placeholder='@("<" +Cares.Web.Resources.Vehicle.Vehicle.InitialOdometer + ">")' data-bind="value:initialOdemeter , validationTooltip:initialOdemeter " />
            </div>
            <label for="currentOdemeter" class="control-label h5 col-sm-2">@Cares.Web.Resources.Vehicle.Vehicle.CurrentOdometer * </label>
            <div class="col-md-2">
                <input id="currentOdemeter" type="text" class="form-control" placeholder='@("<" +Cares.Web.Resources.Vehicle.Vehicle.CurrentOdometer + ">")' data-bind="value:currentOdemeter , validationTooltip:currentOdemeter " />
            </div>
        </div>
        <div class="form-group">
            <label for="color" class="control-label h5 col-sm-2">@Cares.Web.Resources.Vehicle.Vehicle.BranchOwner *</label>
            <div class="col-md-2">
                <input type="checkbox" class="form-control" data-bind="checked: branchOwnerIsChecked " />
            </div>
            <label for="registrationDate" class="control-label h5 col-sm-2">@Cares.Web.Resources.Vehicle.Vehicle.RegisterationDate  </label>
            <div class="col-md-2">
                <input id="registrationDate" type="text" class="form-control" placeholder='@("<" +Cares.Web.Resources.Vehicle.Vehicle.RegisterationDate + ">")' data-bind="datepicker: registrationDate, validationTooltip: registrationDate" />
            </div>
            <label for="registrationExpDate" class="control-label h5 col-sm-2">@Cares.Web.Resources.Vehicle.Vehicle.RegisterationExpDate </label>
            <div class="col-md-2">
                <input id="registrationExpDate" type="text" class="form-control" placeholder='@("<" +Cares.Web.Resources.Vehicle.Vehicle.RegisterationExpDate + ">")' data-bind="datepicker: registrationExpDate, validationTooltip: registrationExpDate" />
            </div>
        </div>

        <div class="panel panel-info">
            <!-- Nav tabs -->
            <ul class="nav nav-tabs" role="tablist">
                <li class="active"><a href="#otherDetail" role="tab" data-toggle="tab">@Cares.Web.Resources.Vehicle.Vehicle.OtherDetails</a></li>
                <li><a href="#purchaseInfo" role="tab" data-toggle="tab">@Cares.Web.Resources.Vehicle.Vehicle.PurchaseInfo</a></li>
                <li><a href="#leaseInfo" role="tab" data-toggle="tab">@Cares.Web.Resources.Vehicle.Vehicle.LeaseInfo</a></li>
                <li><a href="#insuranceInfo" role="tab" data-toggle="tab">@Cares.Web.Resources.Vehicle.Vehicle.InsuranceInfo</a></li>
                <li><a href="#maintenanceSchedule" role="tab" data-toggle="tab">@Cares.Web.Resources.Vehicle.Vehicle.MaintenanceSchedule</a></li>
                <li><a href="#checkListItem" role="tab" data-toggle="tab">@Cares.Web.Resources.Vehicle.Vehicle.CheckListItem</a></li>
                <li><a href="#depriciationInfo" role="tab" data-toggle="tab">@Cares.Web.Resources.Vehicle.Vehicle.DepreciationInfo</a></li>
                <li><a href="#disposalInfo" role="tab" data-toggle="tab">@Cares.Web.Resources.Vehicle.Vehicle.DisposalInfo</a></li>
                <li><a href="#uploadImages" role="tab" data-toggle="tab">@Cares.Web.Resources.Vehicle.Vehicle.UploadImage</a></li>
            </ul>
            <!-- Tab panes -->
            <div class="tab-content">
                <br />
                <!-- first tab - Other Details-->
                <div class="tab-pane active" id="otherDetail" data-bind="with: otherVehicleDetail">
                    <div class="form-group">
                        <label for="numberOfDoors" class="control-label h5 col-sm-2">@Cares.Web.Resources.Vehicle.Vehicle.NumberofDoors </label>
                        <div class="col-md-2">
                            <input type="text" id="numberOfDoors" class="form-control" placeholder='@("<" +Cares.Web.Resources.Vehicle.Vehicle.NumberofDoors + ">")' data-bind="value:numberOfDoors, validationTooltip:numberOfDoors" />
                        </div>
                        <label for="engineCC" class="control-label h5 col-sm-2">@Cares.Web.Resources.Vehicle.Vehicle.EngineCC *</label>
                        <div class="col-md-2">
                            <input type="text" id="engineCC" class="form-control" placeholder='@("<" +Cares.Web.Resources.Vehicle.Vehicle.EngineCC + ">")' data-bind="value:horsePowerCc, validationTooltip:horsePowerCc" />
                        </div>
                        <label for="cylinders" class="control-label h5 col-sm-2">@Cares.Web.Resources.Vehicle.Vehicle.NumberOfCylinders</label>
                        <div class="col-md-2">
                            <input type="text" id="cylinders" class="form-control" placeholder='@("<" +Cares.Web.Resources.Vehicle.Vehicle.NumberOfCylinders + ">")' data-bind="value:numberOfCylinders, validationTooltip:numberOfCylinders" />
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="numberOfDoors" class="control-label h5 col-sm-2">@Cares.Web.Resources.Vehicle.Vehicle.AlloyRim </label>
                        <div class="col-md-2">
                            <input type="checkbox" class="form-control" data-bind="checked: isAlloyRim" />
                        </div>
                        <label for="chasisNumber" class="control-label h5 col-sm-2">@Cares.Web.Resources.Vehicle.Vehicle.ChasisNumber *</label>
                        <div class="col-md-2">
                            <input type="text" id="chasisNumber" class="form-control" placeholder='@("<" +Cares.Web.Resources.Vehicle.Vehicle.ChasisNumber + ">")' data-bind="value:chasisNumber, validationTooltip:chasisNumber" />
                        </div>
                        <label for="engineNumber" class="control-label h5 col-sm-2">@Cares.Web.Resources.Vehicle.Vehicle.EngineNumber </label>
                        <div class="col-md-2">
                            <input type="text" id="engineNumber" class="form-control" placeholder='@("<" +Cares.Web.Resources.Vehicle.Vehicle.EngineNumber + ">")' data-bind="value:engineNumber, validationTooltip:engineNumber" />
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="keyCode" class="control-label h5 col-sm-2">@Cares.Web.Resources.Vehicle.Vehicle.KeyCode  </label>
                        <div class="col-md-2">
                            <input type="text" id="keyCode" class="form-control" placeholder='@("<" +Cares.Web.Resources.Vehicle.Vehicle.KeyCode + ">")' data-bind="value:keyCode , validationTooltip:keyCode " />
                        </div>
                        <label for="radioCode" class="control-label h5 col-sm-2">@Cares.Web.Resources.Vehicle.Vehicle.RadioCode </label>
                        <div class="col-md-2">
                            <input type="text" id="radioCode" class="form-control" placeholder='@("<" +Cares.Web.Resources.Vehicle.Vehicle.RadioCode + ">")' data-bind="value:radioCode , validationTooltip:radioCode " />
                        </div>
                        <label for="topSpeed" class="control-label h5 col-sm-2">@Cares.Web.Resources.Vehicle.Vehicle.TopSpeed</label>
                        <div class="col-md-2">
                            <input type="text" id="topSpeed" class="form-control" placeholder='@("<" +Cares.Web.Resources.Vehicle.Vehicle.TopSpeed + ">")' data-bind="value:topSpeed , validationTooltip:topSpeed " />
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="fWheelSize" class="control-label h5 col-sm-2">@Cares.Web.Resources.Vehicle.Vehicle.FrontWheelSize</label>
                        <div class="col-md-2">
                            <input type="text" id="fWheelSize" class="form-control" placeholder='@("<" +Cares.Web.Resources.Vehicle.Vehicle.FrontWheelSize + ">")' data-bind="value:frontWheelSize, validationTooltip:frontWheelSize" />
                        </div>
                        <label for="bWheelSize" class="control-label h5 col-sm-2">@Cares.Web.Resources.Vehicle.Vehicle.BackWheelSize</label>
                        <div class="col-md-2">
                            <input type="text" id="bWheelSize" class="form-control" placeholder='@("<" +Cares.Web.Resources.Vehicle.Vehicle.BackWheelSize + ">")' data-bind="value:backWheelSize  , validationTooltip:backWheelSize" />
                        </div>
                        <label for="accessories" class="control-label h5 col-sm-2">@Cares.Web.Resources.Vehicle.Vehicle.Accessories </label>
                        <div class="col-md-2">
                            <textarea class="form-control" rows="2" placeholder='@("<" +Cares.Web.Resources.Vehicle.Vehicle.Accessories + ">")' id="accessories" data-bind="value: accessories, valueUpdate: 'keyup'"></textarea>
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="interiorDes" class="control-label h5 col-sm-2">@Cares.Web.Resources.Vehicle.Vehicle.InteriorDescription</label>
                        <div class="col-md-2">
                            <textarea class="form-control" rows="2" placeholder='@("<" +Cares.Web.Resources.Vehicle.Vehicle.InteriorDescription + ">")' id="interiorDes" data-bind="value: interiorDescription , valueUpdate: 'keyup'"></textarea>
                        </div>
                    </div>
                </div>
                <!-- second tab - Purchase Info-->
                <div class="tab-pane" id="purchaseInfo" data-bind="with: vehiclePurchaseInfo">
                    <div class="form-group">
                        <label for="purchaseDate" class="control-label h5 col-sm-2">@Cares.Web.Resources.Vehicle.Vehicle.PurchaseDate </label>
                        <div class="col-md-2">
