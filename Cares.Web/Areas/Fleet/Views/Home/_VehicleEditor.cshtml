                        <label for="leasedStartDt" class="control-label h5 col-sm-2">@Cares.Web.Resources.Vehicle.Vehicle.LeasedStartDate </label>
                        <div class="col-md-2">
                            <input type="text" id="leasedStartDt" class="form-control" placeholder='@("<" +Cares.Web.Resources.Vehicle.Vehicle.LeasedStartDate + ">")' data-bind="datepicker: leasedStartDate, validationTooltip: leasedStartDate" />
