                        <div class="col-md-2">
                            <input type="text" id="pOrderNum" class="form-control" placeholder='@("<" +Cares.Web.Resources.Vehicle.Vehicle.PurchaseOrderNumber + ">")' data-bind="value:purchaseOrderNumber, validationTooltip:purchaseOrderNumber" />
                        </div>
                        <label for="pCost" class="control-label h5 col-sm-2">@Cares.Web.Resources.Vehicle.Vehicle.PurchaseCost</label>
                        <div class="col-md-2">
                            <input type="text" id="pCost" class="form-control" placeholder='@("<" +Cares.Web.Resources.Vehicle.Vehicle.PurchaseCost + ">")' data-bind="value:purchaseCost , validationTooltip:purchaseCost " />
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="usedVehicle" class="control-label h5 col-sm-2">@Cares.Web.Resources.Vehicle.Vehicle.UsedVehicle </label>
                        <div class="col-md-2">
                            <input type="checkbox" id="usedVehicle" class="form-control" data-bind="checked: isUsedVehicle " />
                        </div>
                        <label for="pDescription" class="control-label h5 col-sm-2">@Cares.Web.Resources.Vehicle.Vehicle.PurchaseDescription </label>
                        <div class="col-md-2">
                            <textarea class="form-control" rows="2" placeholder='@("<" +Cares.Web.Resources.Vehicle.Vehicle.PurchaseDescription + ">")' id="pDescription" data-bind="value: purchaseDescription , valueUpdate: 'keyup'"></textarea>
                        </div>
                        <label for="bPartner" class="control-label h5 col-sm-2">@Cares.Web.Resources.Vehicle.Vehicle.BusinessPartner </label>
                        <div class="col-md-2">
                            <select id="bPartner" class="form-control" data-bind="options: $root.businessPartners, optionsText: 'BusinessPartnerCodeName', optionsValue: 'BusinessPartnerId', optionsCaption: '&lt;' + '@Cares.Web.Resources.Vehicle.Vehicle.BusinessPartner' + '&gt;', value: bPMainId , validationTooltip: bPMainId "></select>
                        </div>
                    </div>

                </div>
                <!-- third tab - Lease Info-->
