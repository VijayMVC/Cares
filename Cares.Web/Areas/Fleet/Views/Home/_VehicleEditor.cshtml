                    <div class="form-group">
                        <label for="downPay" class="control-label h5 col-sm-2">@Cares.Web.Resources.Vehicle.Vehicle.DownPayment</label>
                        <div class="col-md-2">
                            <input id="downPay" type="text" placeholder='@("<" +Cares.Web.Resources.Vehicle.Vehicle.DownPayment + ">")' class="form-control" data-bind="value:downPayment, validationTooltip:downPayment " />
