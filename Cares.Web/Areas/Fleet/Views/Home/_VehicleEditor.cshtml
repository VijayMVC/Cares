<div class="well col-md-12" id="vehicleEditor" data-bind="visible: isVehicleEditorVisible, with: selectedVehicle" style="display: none;">
    <div class="detailsContainer">
        <h3 class="page-header detailsHeaderSection">Vehicle Detail</h3>
        <button class="btn  btn-info" id="saveBtn" data-bind="click: $root.onSaveVehicle, enable: hasChanges, css: { 'btn-warning': hasChanges }">@Cares.Web.Resources.Shared.Common.Save</button>
        <button class="btn btn-default" id="saveBtn" data-bind="click: $root.closeVehicleEditor">@Cares.Web.Resources.Shared.Common.Cancel</button>
    </div>
    <form class="form-horizontal">
        <div class="form-group">
            <label for="plateNumber" class="control-label h5 col-sm-2">Plate Number *</label>
            <div class="col-md-2">
                <input type="text" class="form-control" placeholder='<Plate Number>' id="plateNumber" data-bind="value:plateNumber , validationTooltip:plateNumber " />
            </div>
            <label for="vName" class="control-label h5 col-sm-2">Vehicle Name </label>
            <div class="col-md-2">
                <input type="text" class="form-control" placeholder='<Vehicle Name>' id="vName" data-bind="value:vehicleName , validationTooltip:vehicleName " />
            </div>
            <label for="vPictorialCondition" class="control-label h5 col-sm-2">Vehicle Pictorial Condition </label>
            <div class="col-md-2">
                <input type="button" class="form-control" placeholder='<Plate Number>' id="vPictorialCondition" />
            </div>
        </div>
        <div class="form-group">
            <label for="company" class="control-label h5 col-sm-2">Company </label>
            <div class="col-md-2">
                <select id="company" class="form-control" data-bind="options: $root.companies, optionsText: 'CompanyCodeName', optionsValue: 'CompanyId', optionsCaption:  'Company' , value: companyId, validationTooltip: companyId, event:{change:$root.onSelectedCompany}"></select>
            </div>
            <label for="department" class="control-label h5 col-sm-2">Department </label>
            <div class="col-md-2">
                <select id="department" class="form-control" data-bind="options: $root.filteredDepartments, optionsText: 'DepartmentCodeName', optionsValue: 'DepartmentId', optionsCaption:  'Department' , value: departmentId, validationTooltip: departmentId, event:{change:$root.onSelectedDepartemnt}"></select>
            </div>
            <label for="operation" class="control-label h5 col-sm-2">Operation </label>
            <div class="col-md-2">
                <select id="operation" class="form-control" data-bind="options: $root.filteredOperations, optionsText: 'OperationCodeName', optionsValue: 'OperationId', optionsCaption: 'Operation '  , value: operationId, validationTooltip: operationId"></select>
            </div>
        </div>
        <div class="form-group">
            <label for="region" class="control-label h5 col-sm-2">Region </label>
            <div class="col-md-2">
                <select id="region" class="form-control" data-bind="options: $root.regions, optionsText: 'RegionCodeName', optionsValue: 'RegionId', optionsCaption:  'Region' , value: regionId, validationTooltip: regionId "></select>
            </div>
            <label for="fleetPool" class="control-label h5 col-sm-2">Fleet Pool </label>
            <div class="col-md-2">
                <select id="fleetPool" class="form-control" data-bind="options: $root.fleetPools, optionsText: 'FleetPoolCodeName', optionsValue: 'FleetPoolId', optionsCaption:  'Fleet Pool ' , value: fleetPoolId, validationTooltip: fleetPoolId"></select>
            </div>
            <label for="location" class="control-label h5 col-sm-2"> At location  </label>
            <div class="col-md-2">
                <select id="location" class="form-control" data-bind="options: $root.locations, optionsText: 'LocationCode', optionsValue: 'OperationsWorkPlaceId', optionsCaption: 'Location '  , value: locationId, validationTooltip: locationId"></select>
            </div>
        </div>
        <div class="form-group">
            <label for="fuelType" class="control-label h5 col-sm-2">Fuel Type* </label>
            <div class="col-md-2">
                <select id="fuelType" class="form-control" data-bind="options: $root.fuelTypes, optionsText: 'FuelTypeCodeName', optionsValue: 'FuelTypeId', optionsCaption:  'Fuel Type' , value: fuelTypeId, validationTooltip: fuelTypeId "></select>
            </div>
            <label class="control-label h5 col-sm-2">Fuel Level (%) * </label>
            <div class="col-md-2">

            </div>
            <label for="vMake" class="control-label h5 col-sm-2">Vehicle Make *  </label>
            <div class="col-md-2">
                <select id="vMake" class="form-control" data-bind="options: $root.vehicleMakes, optionsText: 'VehicleMakeCodeName', optionsValue: 'VehicleMakeId', optionsCaption: '<Vehicle Make> '  , value: vehicleMakeId, validationTooltip: vehicleMakeId"></select>
            </div>
        </div>
        <div class="form-group">
            <label for="vModel" class="control-label h5 col-sm-2">Vehicle Model * </label>
            <div class="col-md-2">
                <select id="vModel" class="form-control" data-bind="options: $root.vehicleModels, optionsText: 'VehicleModelCodeName', optionsValue: 'VehicleModeld', optionsCaption: 'Vehicle Model', value:vehicleModelId "></select>
            </div>
            <label for="vCategory" class="control-label h5 col-sm-2">Vehicle Category * </label>
            <div class="col-md-2">
                <select id="vCategory" class="form-control" data-bind="options: $root.vehicleCategories, optionsText: 'VehicleCategoryCodeName', optionsValue: 'VehicleCategoryId', optionsCaption: 'Vehicle Category' , value: vehicleCategoryId "></select>
            </div>
            <label for="modelYear" class="control-label h5 col-sm-2">Model Year *  </label>
            <div class="col-md-2">
                <select id="modelYear" class="form-control" data-bind="options: $root.modelYears, optionsText: 'Text', optionsValue: 'Id', optionsCaption: 'Model Year ', value: modelYear, validationTooltip: modelYear"></select>
            </div>
        </div>
        <div class="form-group">
            <label for="status" class="control-label h5 col-sm-2">Status * </label>
            <div class="col-md-2">
                <select id="status" class="form-control" data-bind="options: $root.vehicleStatuses, optionsText: 'VehicleStatusCodeName', optionsValue: 'VehicleStatusId', optionsCaption: 'Status', value:statusId "></select>
            </div>
            <label for="transmission" class="control-label h5 col-sm-2">Transmission * </label>
            <div class="col-md-2">
                <select id="transmission" class="form-control" data-bind="options: $root.transmissionTypes, optionsText: 'TransmissionTypeCodeName', optionsValue: 'TransmissionTypeId', optionsCaption: 'Transmission Type' , value: transmissionTypeId "></select>
            </div>
            <label for="tankSize" class="control-label h5 col-sm-2">Tank Size (ltrs)* </label>
            <div class="col-md-2">
                <input id="tankSize" type="text" class="form-control" placeholder='<Tank Size (ltrs)>' data-bind="value:tankSize , validationTooltip:tankSize " />
            </div>
        </div>
        <div class="form-group">
            <label for="color" class="control-label h5 col-sm-2">Color *</label>
            <div class="col-md-2">
                <input id="color" type="text" class="form-control" placeholder='<Color>' data-bind="value:color , validationTooltip:color " />
            </div>
            <label for="initialOdemeter" class="control-label h5 col-sm-2">Initial Odometer * </label>
            <div class="col-md-2">
                <input id="initialOdemeter" type="text" class="form-control" placeholder='<Initial Odometer >' data-bind="value:initialOdemeter , validationTooltip:initialOdemeter " />
            </div>
            <label for="currentOdemeter" class="control-label h5 col-sm-2">Current Odometer * </label>
            <div class="col-md-2">
                <input id="currentOdemeter" type="text" class="form-control" placeholder='<Current Odometer>' data-bind="value:currentOdemeter , validationTooltip:currentOdemeter " />
            </div>
        </div>
        <div class="form-group">
            <label for="color" class="control-label h5 col-sm-2">Branch Owner *</label>
            <div class="col-md-2">
                <input type="checkbox" class="form-control" data-bind="checked: branchOwnerIsChecked " />
            </div>
            <label for="registrationDate" class="control-label h5 col-sm-2">Registeration Date  </label>
            <div class="col-md-2">
                <input id="registrationDate" type="text" class="form-control" data-bind="datepicker: registrationDate, validationTooltip: registrationDate" />
            </div>
            <label for="registrationExpDate" class="control-label h5 col-sm-2">Registeration Exp. Date </label>
            <div class="col-md-2">
                <input id="registrationExpDate" type="text" class="form-control" data-bind="datepicker: registrationExpDate, validationTooltip: registrationExpDate" />
            </div>
        </div>

        <div class="panel panel-info">
            <!-- Nav tabs -->
            <ul class="nav nav-tabs" role="tablist">
                <li class="active"><a href="#otherDetail" role="tab" data-toggle="tab">Other Details</a></li>
                <li><a href="#purchaseInfo" role="tab" data-toggle="tab">Purchase Info</a></li>
                <li><a href="#leaseInfo" role="tab" data-toggle="tab">Lease Info</a></li>
                <li><a href="#insuranceInfo" role="tab" data-toggle="tab">Insurance Info</a></li>
                <li><a href="#maintenanceSchedule" role="tab" data-toggle="tab">Maintenance Schedule</a></li>
                <li><a href="#checkListItem" role="tab" data-toggle="tab">CheckList Item</a></li>
                <li><a href="#depriciationInfo" role="tab" data-toggle="tab">Depriciation Info</a></li>
                <li><a href="#disposalInfo" role="tab" data-toggle="tab">Disposal Info</a></li>
                <li><a href="#uploadImages" role="tab" data-toggle="tab">Upload Image</a></li>
            </ul>
            <!-- Tab panes -->
            <div class="tab-content">
                <br />
                <!-- first tab - Other Details-->
                <div class="tab-pane active" id="otherDetail" data-bind="with: otherVehicleDetail">
                    <div class="form-group">
                        <label for="numberOfDoors" class="control-label h5 col-sm-2">Number of Doors </label>
                        <div class="col-md-2">
                            <input type="text" id="numberOfDoors" class="form-control" placeholder='<Number of Doors >' data-bind="value:numberOfDoors, validationTooltip:numberOfDoors" />
                        </div>
                        <label for="engineCC" class="control-label h5 col-sm-2">Engine CC *</label>
                        <div class="col-md-2">
                            <input type="text" id="engineCC" class="form-control" placeholder='<Engine CC>' data-bind="value:horsePowerCc, validationTooltip:horsePowerCc" />
                        </div>
                        <label for="cylinders" class="control-label h5 col-sm-2">No. Of Cylinders</label>
                        <div class="col-md-2">
                            <input type="text" id="cylinders" class="form-control" placeholder='<No. Of Cylinders>' data-bind="value:numberOfCylinders, validationTooltip:numberOfCylinders" />
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="numberOfDoors" class="control-label h5 col-sm-2">Alloy Rim </label>
                        <div class="col-md-2">
                            <input type="checkbox" class="form-control" data-bind="checked: isAlloyRim" />
                        </div>
                        <label for="chasisNumber" class="control-label h5 col-sm-2">Chasis Number *</label>
                        <div class="col-md-2">
                            <input type="text" id="chasisNumber" class="form-control" placeholder='<Chasis Number *>' data-bind="value:chasisNumber, validationTooltip:chasisNumber" />
                        </div>
                        <label for="engineNumber" class="control-label h5 col-sm-2">Engine Number </label>
                        <div class="col-md-2">
                            <input type="text" id="engineNumber" class="form-control" placeholder='<Engine Number>' data-bind="value:engineNumber, validationTooltip:engineNumber" />
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="keyCode" class="control-label h5 col-sm-2">Key Code  </label>
                        <div class="col-md-2">
                            <input type="text" id="keyCode" class="form-control" placeholder='<Key Code  >' data-bind="value:keyCode , validationTooltip:keyCode " />
                        </div>
                        <label for="radioCode" class="control-label h5 col-sm-2">Radio Code </label>
                        <div class="col-md-2">
                            <input type="text" id="radioCode" class="form-control" placeholder='<Radio Code >' data-bind="value:radioCode , validationTooltip:radioCode " />
                        </div>
                        <label for="topSpeed" class="control-label h5 col-sm-2">Top Speed (km/h) </label>
                        <div class="col-md-2">
                            <input type="text" id="topSpeed" class="form-control" placeholder='<Top Speed>' data-bind="value:topSpeed , validationTooltip:topSpeed " />
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="fWheelSize" class="control-label h5 col-sm-2">F/Wheel Size (Inches) </label>
                        <div class="col-md-2">
                            <input type="text" id="fWheelSize" class="form-control" placeholder='<F/Wheel Size>' data-bind="value:frontWheelSize, validationTooltip:frontWheelSize" />
                        </div>
                        <label for="bWheelSize" class="control-label h5 col-sm-2"> 	B/Wheel Size (Inches) </label>
                        <div class="col-md-2">
                            <input type="text" id="bWheelSize" class="form-control" placeholder='<B/Wheel Size>' data-bind="value:backWheelSize  , validationTooltip:backWheelSize" />
                        </div>
                        <label for="accessories" class="control-label h5 col-sm-2">Accessories </label>
                        <div class="col-md-2">
                            <textarea class="form-control" rows="2" placeholder='Accessories' id="accessories" data-bind="value: accessories, valueUpdate: 'keyup'"></textarea>
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="interiorDes" class="control-label h5 col-sm-2">Interior Description</label>
                        <div class="col-md-2">
                            <textarea class="form-control" rows="2" placeholder='Interior Description' id="interiorDes" data-bind="value: interiorDescription , valueUpdate: 'keyup'"></textarea>
                        </div>
                    </div>
                </div>
                <!-- second tab - Purchase Info-->
                <div class="tab-pane" id="purchaseInfo" data-bind="with: vehiclePurchaseInfo">
                    <div class="form-group">
                        <label for="purchaseDate" class="control-label h5 col-sm-2">Purchase Date </label>
                        <div class="col-md-2">
                            <input id="purchaseDate" type="text" placeholder="<Purchase Date>" class="form-control" data-bind="datepicker: purchaseDate , validationTooltip: purchaseDate " />
                        </div>
                        <label for="pOrderNum" class="control-label h5 col-sm-2">Purchase Order Number </label>
                        <div class="col-md-2">
                            <input type="text" id="pOrderNum" class="form-control" placeholder='<Purchase Order Number >' data-bind="value:purchaseOrderNumber, validationTooltip:purchaseOrderNumber" />
                        </div>
                        <label for="pCost" class="control-label h5 col-sm-2">Purchase Cost</label>
                        <div class="col-md-2">
                            <input type="text" id="pCost" class="form-control" placeholder='<Purchase Cost>' data-bind="value:purchaseCost , validationTooltip:purchaseCost " />
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="usedVehicle" class="control-label h5 col-sm-2">Used Vehicle? </label>
                        <div class="col-md-2">
                            <input type="checkbox" id="usedVehicle" class="form-control" data-bind="checked: isUsedVehicle " />
                        </div>
                        <label for="pDescription" class="control-label h5 col-sm-2">Purchase Description </label>
                        <div class="col-md-2">
                            <textarea class="form-control" rows="2" placeholder='<Purchase Description>' id="pDescription" data-bind="value: purchaseDescription , valueUpdate: 'keyup'"></textarea>
                        </div>
                        <label for="bPartner" class="control-label h5 col-sm-2">Business Partner </label>
                        <div class="col-md-2">
                            <select id="bPartner" class="form-control" data-bind="options: $root.businessPartners, optionsText: 'BusinessPartnerCodeName', optionsValue: 'BusinessPartnerId', optionsCaption: 'Business Partner Code', value: bPMainId , validationTooltip: bPMainId "></select>
                        </div>
                    </div>

                </div>
                <!-- third tab - Lease Info-->
                <div class="tab-pane" id="leaseInfo" data-bind="with: vehicleLeasedInfo">
                    <div class="form-group">
                        <label for="downPay" class="control-label h5 col-sm-2">Down Payment</label>
                        <div class="col-md-2">
                            <input id="downPay" type="text" placeholder="<Down Payment>" class="form-control" data-bind="value:downPayment, validationTooltip:downPayment " />
                        </div>
                        <label for="leasedStartDt" class="control-label h5 col-sm-2">Leased Start Date </label>
                        <div class="col-md-2">
                            <input type="text" id="leasedStartDt" class="form-control" placeholder='<Leased Start Date >' data-bind="datepicker: leasedStartDate, validationTooltip: leasedStartDate" />
                        </div>
                        <label for="leasedFinishDt" class="control-label h5 col-sm-2">Leased Finish Date</label>
                        <div class="col-md-2">
                            <input type="text" id="leasedFinishDt" class="form-control" placeholder='<Leased Finish Date>' data-bind="datepicker: leasedFinishDate, validationTooltip: leasedFinishDate" />
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="leasedOwnerShip" class="control-label h5 col-sm-2">Leased Ownership? </label>
                        <div class="col-md-2">
                            <input type="checkbox" id="leasedOwnerShip" class="form-control" data-bind="checked: leaseToOwnership  " />
                        </div>
                        <label for="monthlyPayment" class="control-label h5 col-sm-2">Monthly Payment </label>
                        <div class="col-md-2">
                            <input type="text" id="monthlyPayment" class="form-control" placeholder='<Monthly Payment>' data-bind="value:monthlyPayment , validationTooltip:monthlyPayment " />
                        </div>
                        <label for="insuranceRt" class="control-label h5 col-sm-2">Interest Rate (%)</label>
                        <div class="col-md-2">
                            <input type="text" id="insuranceRt" class="form-control" placeholder='<Interest Rate>' data-bind="value:interestRate , validationTooltip:interestRate " />
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="principalPayment" class="control-label h5 col-sm-2">Prinicipal Payment</label>
                        <div class="col-md-2">
                            <input type="text" id="principalPayment" class="form-control" placeholder='<Prinicipal Payment>' data-bind="value:prinicipalPayment , validationTooltip:prinicipalPayment " />
                        </div>
                        <label for="firstPaymentDt" class="control-label h5 col-sm-2">First Payment Date </label>
                        <div class="col-md-2">
                            <input type="text" id="firstPaymentDt" class="form-control" placeholder='<First Payment Date >' data-bind="datepicker: firstPaymentDate , validationTooltip: firstPaymentDate " />
                        </div>
                        <label for="firstPayment" class="control-label h5 col-sm-2">First Payment</label>
                        <div class="col-md-2">
                            <input type="text" id="firstPayment" class="form-control" placeholder='<First Payment>' data-bind="value:firstPaymentDate , validationTooltip:firstPaymentDate " />
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="lastPayment" class="control-label h5 col-sm-2">Last Payment </label>
                        <div class="col-md-2">
                            <input type="text" id="lastPayment" class="form-control" placeholder='<Last Payment>' data-bind="value:firstMonthPayment , validationTooltip:firstMonthPayment " />
                        </div>
                        <label for="bPartner" class="control-label h5 col-sm-2">Business Partner </label>
                        <div class="col-md-2">
                            <select id="bPartner" class="form-control" data-bind="options: $root.businessPartners, optionsText: 'BusinessPartnerCodeName', optionsValue: 'BusinessPartnerId', optionsCaption: 'Business Partner Code', value: bPMainId , validationTooltip: bPMainId"></select>
                        </div>
                    </div>
                </div>
                <!-- fourth tab - Insuran Info-->
                <div class="tab-pane" id="insuranceInfo" data-bind="with: vehicleInsuranceInfo">
                    <div class="form-group">
                        <label for="insAgent" class="control-label h5 col-sm-2">Insurance Agent </label>
                        <div class="col-md-2">
                            <input id="insAgent" type="text" placeholder="<Insurance Agent>" class="form-control" data-bind="value:insuranceAgent  , validationTooltip:insuranceAgent  " />
                        </div>
                        <label for="insStartDt" class="control-label h5 col-sm-2">Insurance Start Date </label>
                        <div class="col-md-2">
                            <input type="text" id="insStartDt" class="form-control" placeholder='<Insurance Start Date >' data-bind="datepicker: insuranceDate, validationTooltip: insuranceDate" />
                        </div>
                        <label for="insRenewalDt" class="control-label h5 col-sm-2">Insurance Renewal Date </label>
                        <div class="col-md-2">
                            <input type="text" id="insRenewalDt" class="form-control" placeholder='<Insurance Renewal Date >' data-bind="datepicker: renewalDate , validationTooltip: renewalDate " />
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="coverageLimit" class="control-label h5 col-sm-2">Coverage Limit</label>
                        <div class="col-md-2">
                            <input id="coverageLimit" type="text" placeholder="<Coverage Limit>" class="form-control" data-bind="value:coverageLimit  , validationTooltip:coverageLimit  " />
                        </div>
                        <label for="premium" class="control-label h5 col-sm-2">Premium </label>
                        <div class="col-md-2">
                            <input type="text" id="premium" class="form-control" placeholder='<Premium >' data-bind="value:premium  , validationTooltip:premium  " />
                        </div>
                        <label for="insValue" class="control-label h5 col-sm-2">Insured Value</label>
                        <div class="col-md-2">
                            <input type="text" id="insValue" class="form-control" placeholder='<Insured Value>' data-bind="value:insuredValue   , validationTooltip:insuredValue   " />
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="insuranceType" class="control-label h5 col-sm-2">Insurance Type *</label>
                        <div class="col-md-2">
                            <select id="insuranceType" class="form-control" data-bind="options: $root.insuranceType, optionsText: 'InsuranceTypeCodeName', optionsValue: 'InsuranceTypeId', optionsCaption: '<Insurance Type>' , value: insuranceTypeId  , validationTooltip: insuranceTypeId "></select>
                        </div>
                        <label for="bPartnerInsuranceInfo" class="control-label h5 col-sm-2">Business Partner </label>
                        <div class="col-md-2">
                            <select id="bPartnerInsuranceInfo" class="form-control" data-bind="options: $root.businessPartners, optionsText: 'BusinessPartnerCodeName', optionsValue: 'BusinessPartnerId', optionsCaption: '<Business Partner Code>' , value: bPMainId , validationTooltip: bPMainId"></select>
                        </div>
                    </div>
                </div>
                <!-- fifth tab - Maintenance Schedule-->
                <div class="tab-pane" id="maintenanceSchedule">
                    <div class="form-group">
                        <label for="maintStartDt" class="control-label h5 col-sm-2">Maintenance Start Date *</label>
                        <div class="col-md-2">
                            <input id="maintStartDt" type="text" placeholder="<Maintenance Start Date >" class="form-control">
                        </div>
                        <label for="frequencyDays" class="control-label h5 col-sm-2">Frequency (Days) </label>
                        <div class="col-md-2">
                            <input type="text" id="frequencyDays" class="form-control" placeholder='<Frequency (Days) >' />
                        </div>
                        <label for="frequencyKm" class="control-label h5 col-sm-2">Frequency KM </label>
                        <div class="col-md-2">
                            <input type="text" id="frequencyKm" class="form-control" placeholder='<Frequency KM>' />
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="maintainceType" class="control-label h5 col-sm-2">Maintenance Type *</label>
                        <div class="col-md-2">
                            <select id="maintainceType" class="form-control" data-bind="options: $root.maintenanceTypes, optionsText: 'MaintenanceTypeCodeName', optionsValue: 'MaintenanceTypeId', optionsCaption: '<Maintenance Type>' "></select>
                        </div>
                    </div>
                </div>
                <!-- sixth tab - Check List item-->
                <div class="tab-pane" id="checkListItem">
                    <div class="form-group">
                        <label for="vCheckList" class="control-label h5 col-sm-2">Vehicle Check List *</label>
                        <div class="col-md-2">
                            <select id="vCheckList" class="form-control" data-bind="options: $root.vehicleCheckList, optionsText: 'VehicleCheckListCodeName', optionsValue: 'VehicleCheckListId', optionsCaption: '<Vehicle Check List>' "></select>
                        </div>
                    </div>
                </div>
                <!-- seventh tab - Depriciation Info-->
                <div class="tab-pane" id="depriciationInfo" data-bind="with: vehicleDepreciation">
                    <div class="form-group">
                        <label for="upStartDt" class="control-label h5 col-sm-2">Useful Period Start Date </label>
                        <div class="col-md-2">
                            <input id="upStartDt" type="text" placeholder="<Useful Period Start Date >" class="form-control" data-bind="datepicker: usefulPeriodStartDate , validationTooltip: usefulPeriodStartDate " />
                        </div>
                        <label for="upEndDt" class="control-label h5 col-sm-2">Useful Period End Date </label>
                        <div class="col-md-2">
                            <input type="text" id="upEndDt" class="form-control" placeholder='<Useful Period End Date >' data-bind="datepicker: usefulPeriodEndDate , validationTooltip: usefulPeriodEndDate " />
                        </div>
                        <label for="firstMonthDep" class="control-label h5 col-sm-2">First Month Dep.</label>
                        <div class="col-md-2">
                            <input type="text" id="firstMonthDep" class="form-control" placeholder='<First Month Dep.>' data-bind="value:firstMonthDepAmount, validationTooltip:firstMonthDepAmount" />
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="monthlyDep" class="control-label h5 col-sm-2">Monthly Dep.</label>
                        <div class="col-md-2">
                            <input id="monthlyDep" type="text" placeholder="<Monthly Dep.>" class="form-control" data-bind="value:monthlyDepAmount, validationTooltip:monthlyDepAmount" />
                        </div>
                        <label for="lastMonthDep" class="control-label h5 col-sm-2">Last Month Dep.</label>
                        <div class="col-md-2">
                            <input type="text" id="lastMonthDep" class="form-control" placeholder='<Last Month Dep.>' data-bind="value:lastMonthDepAmount, validationTooltip:lastMonthDepAmount" />
                        </div>
                        <label for="residualVal" class="control-label h5 col-sm-2">Residual Value </label>
                        <div class="col-md-2">
                            <input type="text" id="residualVal" class="form-control" placeholder='<Residual Value >' data-bind="value:residualValue, validationTooltip:residualValue" />
                        </div>
                    </div>

                </div>
                <!-- eight tab - Disposal Info-->
                <div class="tab-pane" id="disposalInfo" data-bind="with: vehicleDisposalInfo">
                    <div class="form-group">
                        <label for="dispDt" class="control-label h5 col-sm-2">Disposal Date</label>
                        <div class="col-md-2">
                            <input id="dispDt" type="text" placeholder="<Disposal Date>" class="form-control" data-bind="datepicker: saleDate  , validationTooltip: saleDate  " />
                        </div>
                        <label for="dsipCost" class="control-label h5 col-sm-2">Disposal Cost </label>
                        <div class="col-md-2">
                            <input type="text" id="dsipCost" class="form-control" placeholder='<Disposal Cost >' data-bind="value:salePrice , validationTooltip:salePrice " />
                        </div>
                        <label for="bPartnerInsuranceInfo" class="control-label h5 col-sm-2">Business Partner </label>
                        <div class="col-md-2">
                            <select id="bPartnerInsuranceInfo" class="form-control" data-bind="options: $root.businessPartners, optionsText: 'BusinessPartnerCodeName', optionsValue: 'BusinessPartnerId', optionsCaption: '<Business Partner Code>',value:bPMainId "></select>
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="disposalDes" class="control-label h5 col-sm-2">Disposal Description</label>
                        <div class="col-md-2">
                            <textarea class="form-control" rows="2" placeholder='<Disposal Description>' id="disposalDes" data-bind="value: disposalDescription, valueUpdate: 'keyup'"></textarea>
                        </div>
                    </div>
                </div>
                <!-- ninth tab - Upload Image-->
                <div class="tab-pane" id="uploadImages">
                </div>

            </div>
        </div>
    </form>

</div>