<div id="raVehicleCheckListOutDialog" class="modal fade" tabindex="-1" role="dialog" aria-labelledby="raVehicleCheckListOutDialogModalLabel" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">@Cares.Web.Resources.Shared.Common.Close</span></button>
                <h4 class="modal-title" id="myModalLabel">CheckList Out</h4>
            </div>
            <div class="modal-body">
                <div>
                    <form class="form-horizontal">
                        <div>
                            <table class="table table-condensed">
                                <thead>
                                    <tr>
                                        <th></th>
                                        <th>CheckList</th>
                                        <th>Is Interior</th>
                                        <th>Comments</th>
                                    </tr>
                                </thead>
                                <tbody data-bind="foreach: $root.selectedRaHireGroup().raVehicleCheckListsOut">
                                    <tr>
                                        <td>
                                            <input type="checkbox" data-bind="checked: isSelected" />
                                        </td>
                                        <td data-bind="text: vehicleCheckListCodeName"></td>
                                        <td data-bind="text: isInterior">
                                        <td data-bind="text: comments"></td>
                                    </tr>
                                </tbody>
                            </table>
                        </div>
                    </form>
                </div>
            </div>
            <div class="modal-footer">
            </div>
        </div>
    </div>
</div>
