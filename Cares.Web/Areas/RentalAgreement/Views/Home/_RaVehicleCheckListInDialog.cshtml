<div id="raVehicleCheckListInDialog" class="modal fade" tabindex="-1" role="dialog" aria-labelledby="raVehicleCheckListInDialogModalLabel" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <h2 class="modal-title" id="myModalLabel">CheckList In</h2>
            </div>
            <div class="modal-body">
                <div class="row">
                    <div class="well">
                        <table class="table table-condensed">
                            <thead>
                                <tr>
                                    <th></th>
                                    <th>Name</th>
                                    <th>Is Interior</th>
                                    <th>Comments</th>
                                </tr>
                            </thead>
                            <tbody>
                                <!-- ko foreach: $root.selectedRaHireGroup().raVehicleCheckListsIn -->
                                <tr>
                                    <td>
                                        <input type="checkbox" data-bind="checked: isSelected" />
                                    </td>
                                    <td data-bind="text: vehicleCheckListCodeName"></td>
                                    <td data-bind="text: isInterior">
                                    <td data-bind="text: comments"></td>
                                </tr>
                                <!-- /ko -->
                                <!-- ko if: $root.selectedRaHireGroup().raVehicleCheckListsIn().length === 0 -->
                                <tr>
                                    <td>&nbsp;</td>
                                    <td colspan="3">No record(s) to display yet. </td>
                                </tr>
                                <!-- /ko -->
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn" data-dismiss="modal">@Cares.Web.Resources.Shared.Common.Close</button>
            </div>
        </div>
    </div>
</div>
