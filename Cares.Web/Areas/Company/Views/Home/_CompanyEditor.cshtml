
<div class="well col-md-12" id="CompanyEditor" data-bind="visible: isCompanyEditorVisible , with:selectedCompany" style="display: none;">
    <div class="float-left detailsContainer">
        <h3 class="page-header detailsHeaderSection">Company Details</h3>
        <button class="btn  btn-info" data-bind="click:$root.onSaveCompany">Save</button>
        <button class="btn  btn-default" data-bind="click:$root.onCancelCompanySave">Cancel</button>
    </div>
    <form class="form-horizontal">
        <div class="form-group">
            <label class="control-label h5 col-sm-2">Company Code</label>
            <div class="col-md-2">
                <input type="text" class="form-control" placeholder="<Company Code>" id="tariffTypeCode" data-bind="value: companyCode" />
            </div>
            <label class="control-label h5 col-sm-2">Company Name</label>
            <div class="col-md-2">
                <input type="text" class="form-control" placeholder="<Company Name>" id="tariffTypeName" data-bind="value:companyName" />
            </div>
            <label class="control-label h5 col-sm-2">Description</label>
            <div class="col-md-2">
                <input type="text" class="form-control" placeholder="<Description>" id="tariffTypeDescription" data-bind="value:companyDescription" />
            </div>
        </div>
        <div class="form-group">
            <label class="control-label h5 col-sm-2">Organization Group</label>
            <div class="col-md-2">
                <select class="form-control" data-bind="options: $root.orgGroupList, optionsText: 'OrgGroupCode', optionsValue: 'OrgGroupId', optionsCaption: '<Org Group>', value:orgGroupId "></select>
            </div>
            <label class="control-label h5 col-sm-2">Business Segment</label>
            <div class="col-md-2">
                <select class="form-control" data-bind="options: $root.businessSegList, optionsText: 'BusinessSegmentCodeName', optionsValue: 'BusinessSegmentId', optionsCaption: '<Business Segment>', value:businessSegmentId"></select>
            </div>
            <label class="control-label h5 col-sm-2">Parent Company</label>
            <div class="col-md-2">
                <select class="form-control" data-bind="options: $root.filteredCompanyList, optionsText: 'CompanyCodeName', optionsValue: 'CompanyId', optionsCaption: '<Parent Company>' , value:parentCompanyId"></select>
            </div>
        </div>
        <div class="form-group">
            <label class="control-label h5 col-sm-2">NTN</label>
            <div class="col-md-2">
                <input type="text" class="form-control" data-bind="value:ntn" placeholder="<NTN>" />
            </div>

            <label class="control-label h5 col-sm-2">UAN</label>
            <div class="col-md-2">
                <input type="text" class="form-control" data-bind="value:uan" placeholder="<UAN>"/>
            </div>
            <label class="control-label h5 col-sm-2">Legal Name</label>
            <div class="col-md-2">
                <input type="text" class="form-control" data-bind="value:CompanyLegalName" placeholder="<Legal Name>" />
            </div>
        </div>
        <div class="form-group">
            <label class="control-label h5 col-sm-2">Paid-up Capital ($) </label>
            <div class="col-md-2">
                <input type="text" class="form-control" data-bind="value:paidUpCapital" placeholder="<Paid-up Capital>" />
            </div>
            <label class="control-label h5 col-sm-2">Cr Number </label>
            <div class="col-md-2">
                <input type="text" class="form-control" data-bind="value:crNumber" placeholder="<Cr Number>" />
            </div>
       
    </form>

</div>
